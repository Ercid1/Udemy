#include <stdio.h>

int main()
{
    int vector[] = {4, 7, 9, 75, -5, 6, 12, -18, 0, 1};
    int numElements = 10;
    int i;
    int min, max;

    min = vector[0];
    max = vector[0];

    for (i = 1; i < numElements; i++)
    {
        if (vector[i] < min)
        {
            min = vector[i];
        }
        if (vector[i] > max)
        {
            max = vector[i];
        }
    }

    printf("Valor mínimo: %d\n", min);
    printf("Valor máximo: %d\n", max);

    return 0;
}
