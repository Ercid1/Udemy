#include <stdio.h>

int main()
{
    float vector[5];
    float *puntero = vector;

    printf("Ingrese 5 n√∫meros reales:\n");

    for (int i = 0; i < 5; i++)
    {
        scanf("%f", &vector[i]);
    }

    float suma = 0;
    for (int i = 0; i < 5; i++)
    {
        suma += *(puntero + i);
    }

    float media = suma / 5;

    printf("La media de los elementos del vector es: %.2f\n", media);

    return 0;
}
